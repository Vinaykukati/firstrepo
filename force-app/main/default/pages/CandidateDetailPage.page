<apex:page showHeader="true" sidebar="false" standardController="Candidate__c">
<apex:form >
 <apex:sectionHeader title="Candidate Details" subtitle="Candidate.Name"/>
 <apex:pageBlock >
  <apex:pageBlockButtons location="top">
  <apex:commandButton value="Edit"/>
  <apex:commandButton value="Delete"/>
  <apex:commandButton value="Clone"/>
  </apex:pageBlockButtons>
  <apex:pageBlockSection title="Candidate Details" collapsible="false" columns="1">
    <apex:outputField value="{!Candidate__c.Name}"/>
    <apex:Outputfield value="{!Candidate__c.First_Name__c}"/>
    <apex:outputField value="{!Candidate__c.Last_Name__c}"/>
    <apex:outputField value="{!Candidate__c.Zip_PostalCode__c}"/>
    <apex:outputField value="{!Candidate__c.Country__c}"/>
    <apex:outputField value="{!Candidate__c.SSN__c}"/>
    <apex:outputField value="{!Candidate__c.Gmap__c}"/>
  </apex:pageBlockSection>
    <apex:pageBlockSection title="Address">
      <apex:outputField value="{!Candidate__c.Street__c}"/>
      <apex:outputField value="{!Candidate__c.State_Province__c}"/>
      <apex:outputField value="{!Candidate__c.City__c}"/> <br/>
      <apex:outputField value="{!Candidate__c.Fax__c}"/>
      <apex:outputField value="{!Candidate__c.Email__c}"/>
    </apex:pageBlockSection>
      <apex:pageBlockSection title="Employement">
        <apex:outputField value="{!Candidate__c.Current_Employed__c}"/>
        <apex:outputField value="{!Candidate__c.Years_of_Experience__c}"/>
        <apex:outputField value="{!Candidate__c.Current_Employer__c}"/>
      </apex:pageBlockSection>
        <apex:pageBlockSection title="Additional Details">
          <apex:outputField value="{!Candidate__c.US_Citizen__c}"/>
          <apex:outputField value="{!Candidate__c.Education__c}"/>
          <apex:outputField value="{!Candidate__c.Visa_Required__c}"/>
        </apex:pageBlockSection>
 </apex:pageBlock>
</apex:form>
</apex:page>